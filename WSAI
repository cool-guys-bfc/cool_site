      "cell_type": "code",
      "source": [
        "class Frame(object):\n",
        "  def __init__(self,items : list):\n",
        "    self.items=list(items)\n",
        "  def get(self,index):\n",
        "    return(self.itemsitems[index])\n",
        "  def set(self,index,val):\n",
        "    self.items[index]=val\n",
        "  def delete(self,index):\n",
        "    del self.items[index]\n",
        "  def remove(self,item):\n",
        "    self.items.remove(item)\n",
        "  def append(self,item):\n",
        "    self.items.append(item)\n",
        "  def get_all(self):\n",
        "    results=[]\n",
        "    for item in self.items:\n",
        "      if type(item)==Frame:\n",
        "          results.append(item.items)\n",
        "      else:\n",
        "        results.append(item)\n",
        "    return(results)\n",
        "  def stringify(self):\n",
        "    return(str(self.get_all()))\n",
        "\n",
        "class DictFrame(object):\n",
        "  def __init__(self,items : dict):\n",
        "    self.items=dict(items)\n",
        "  def get(self,index):\n",
        "    return(self.items[index])\n",
        "  def set(self,index,val):\n",
        "    self.items[index]=val\n",
        "  def delete(self,index):\n",
        "    del self.items[index]\n",
        "  def stringify(self):\n",
        "    return(str(self.items))\n",
        "#Bugs:\n",
        "#Stringify and Get All for Frame don't work for 3D or higher dimensions\n",
        "#DictFrame does not have Get All or work\n",
        "from bs4 import BeautifulSoup\n",
        "import requests as ws\n",
        "import IPython.display as ipd\n",
        "import time\n",
        "db={\"scripts\":[],\"images\":[]}\n",
        "urls=[\n",
        "      \"https://docs.mrchromebox.tech/docs/getting-started.html\",\n",
        "      \"https://developer.mozilla.org/en-US/docs/Web/API\",\n",
        "      \"https://developer.mozilla.org/en-US/play\",\n",
        "      \"https://app.arduino.cc/\"\n",
        "]\n",
        "def process():\n",
        "  handlers=[]\n",
        "  for url in urls:\n",
        "    handlers.append(BeautifulSoup(ws.get(url).text,\"html.parser\"))\n",
        "  return(handlers)\n",
        "i=0\n",
        "for soup in process():\n",
        "  #print(\"URL:\"+urls[i])\n",
        "  #print(\"Title: \"+soup.title.text)\n",
        "  #print(\"IMAGES\")\n",
        "  for tag in soup.find_all(\"img\"):\n",
        "    result=[]\n",
        "    #print(\"IMG(\"+\"IMG.ALT:\"+tag[\"alt\"]+\",IMG.SRC:\"+tag[\"src\"]+\")\")\n",
        "    #print(urls[i]+tag[\"src\"])\n",
        "    result.append(\"ALT:\"+tag[\"alt\"])\n",
        "    result.append(\"SRC:\"+urls[i]+tag[\"src\"])\n",
        "    db[\"images\"].append(result)\n",
        "  #print(\"SCRIPTS\")\n",
        "  for tag in soup.find_all(\"script\"):\n",
        "    result=[]\n",
        "    #print(\"Javascript(\"+tag.text+\")\")\n",
        "    result.append(\"TEXT:\"+tag.text)\n",
        "    if \"src\" in tag.attrs:\n",
        "      #print(\"SCRIPT.SRC: \"+tag[\"src\"])\n",
        "      result.append(\"SRC:\"+tag[\"src\"])\n",
        "    if \"type\" in tag.attrs:\n",
        "      #print(\"SCRIPT.TYPE: \"+tag[\"type\"])\n",
        "      result.append(\"TYPE:\"+tag[\"type\"])\n",
        "    db[\"scripts\"].append(result)\n",
        "  i+=1\n",
        "for item in db[\"scripts\"]:\n",
        "  pass\n",
        "google=\"https://datasetsearch.research.google.com/search?query=\"\n",
        "#display(ipd.HTML('''\n",
        "#<embed src=\"https://projects.fivethirtyeight.com/soccer-predictions/\" width=\"500\" height=\"500\"/>\n",
        "#'''))\n",
        "#print(ws.get(\"https://www.ssa.gov/oact/babynames/names.zip/yob2015.txt/\").text)"
