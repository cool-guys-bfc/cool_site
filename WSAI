      "cell_type": "code",
      "source": [
        "import requests as ws\n",
        "from bs4 import *\n",
        "import string\n",
        "import IPython.display as ipd\n",
        "urls=[\n",
        "    \"html5gameengine.com\",\n",
        "    \"web.dev/learn/html/\",\n",
        "    \"developer.mozilla.org/en-US/docs/Web/HTML\"\n",
        "]\n",
        "db={\"htmlgameengine.doc\":\"\",\"html.doc\":\"\",\"js.doc\":\"\"}\n",
        "def collect(s):\n",
        "  title=s.title.text.lower().split(\" \")\n",
        "  if (\"html\" in title or \"html5\" in title) and (\"game\" in title) and (\"engine\" in title or \"engines\" in title):\n",
        "    db[\"htmlgameengine.doc\"]+=s.body.text\n",
        "  elif \"html\" in title or \"html5\" in title or \"mdn\" in title:\n",
        "    db[\"html.doc\"]+=s.body.text\n",
        "  else:\n",
        "    pass\n",
        "def process():\n",
        "  handlers=[]\n",
        "  for url in urls:\n",
        "    handlers.append(BeautifulSoup(ws.get(\"https://\"+url).text,\"html.parser\"))\n",
        "  return(handlers)\n",
        "for soup in process():\n",
        "  collect(soup)\n",
        "q=input(\"Enter topic: \").lower()\n",
        "qs=q.split(\" \")\n",
        "if (\"html\" in qs or \"html5\" in qs) and (\"game\" in qs) and (\"engine\" in qs or \"engines\" in qs):\n",
        "  display(ipd.HTML(db[\"htmlgameengine.doc\"]))\n",
        "elif (\"html\" in qs) or (\"html5\" in qs):\n",
        "  display(ipd.HTML(db[\"html.doc\"]))\n",
        "else:\n",
        "  pass"
      ],
